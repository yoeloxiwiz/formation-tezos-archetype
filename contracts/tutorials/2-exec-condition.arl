archetype exec_condition

variable value : nat = 0

variable admin : address = tz1h4CiqWxNe4UxSpkwXy617RM6DaK6NU76P

entry set_admin(addr : address) {
    admin := addr
}

entry main(v : nat) {
  called by admin
  require {
      r1: 10 <= v < 20;
      r2: v % 2 = 0 otherwise "EXPECTED EVEN VALUE"
  }
  effect {
     value := v;
  }
}